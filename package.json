{
  "name": "spark-mediamanager",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Gabriel Duarte <gabrielqduarte@gmail.com> (https://github.com/GabrielDuarteM/)",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --env.env=dev",
    "test-ci": "jest",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "yarn lint-ts && yarn lint-prettier",
    "lint-fix": "yarn lint-ts-fix && yarn lint-prettier-fix",
    "lint-ts": "tslint 'src/**/*.ts' '*.ts' -e 'src/**/*.d.ts'",
    "lint-ts-fix": "tslint 'src/**/*.ts' '*.ts' --fix -e 'src/**/*.d.ts'",
    "lint-prettier": "prettier --debug-check -l --no-semi --print-width 120 --trailing-comma es5 --parser typescript \"**/*.ts\"",
    "lint-prettier-fix": "prettier -l --no-semi --print-width 120 --trailing-comma es5 --parser typescript \"**/*.ts\"",
    "build": "webpack --env.env=prod",
    "build-dev": "webpack --env.env=dev",
    "build-ci": "yarn test-ci && yarn lint && yarn build-dev && yarn build",
    "storybook": "start-storybook -p 6006",
    "sb": "yarn storybook",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": ["ts", "tsx", "js"]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "material-ui": "1.0.0-beta.4",
    "material-ui-icons": "^1.0.0-alpha.19",
    "material-ui-legacy": "npm:material-ui",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "typeface-roboto": "^0.0.35",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.3",
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^2.8.4",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^20.0.5",
    "@types/node": "^8.0.19",
    "@types/react": "^15.6.1",
    "@types/react-dom": "^15.5.1",
    "@types/react-hot-loader": "^3.0.3",
    "@types/react-intl": "^2.3.1",
    "@types/react-redux": "^5.0.0",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/recompose": "^0.23.3",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.0.1",
    "@types/storybook__react": "^3.0.3",
    "@types/uuid": "^3.4.0",
    "@types/webpack": "^3.0.5",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.2.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.10",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.1.0",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
