{
  "name": "spark-mediamanager",
  "version": "0.1.0",
  "main": "src/main.js",
  "author": "Gabriel Duarte <gabrielqduarte@gmail.com> (https://github.com/GabrielDuarteM/)",
  "license": "MIT",
  "scripts": {
    "start":
      "concurrently --kill-others --names \"WEBPACK,ELECTRON\" -s first \"yarn start-dev-server\" \"yarn start-electron\"",
    "start-electron": "cross-env NODE_ENV=development electron .",
    "start-dev-server": "webpack-dev-server --env.env=dev",
    "test-ci": "jest --coverage",
    "test-wcov": "jest --watch --coverage",
    "test-cov": "jest --coverage",
    "test": "jest --watch",
    "lint": "yarn lint-ts && yarn lint-prettier",
    "lint-ts": "tslint 'src/**/*.{ts|tsx}' '*.{ts|tsx}' -e 'src/**/*.d.ts'",
    "lint-prettier-fix": "yarn lint-prettier --write",
    "lint-prettier": "prettier -l \"**/*.{tsx,ts,js,json}\"",
    "build": "webpack --env.env=prod",
    "build-dev": "webpack --env.env=dev",
    "build-ci": "yarn test && yarn lint && yarn build-dev && yarn build && codecov",
    "storybook": "start-storybook -p 6006",
    "sb": "yarn storybook",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "mapCoverage": true,
    "testURL": "http://localhost"
  },
  "dependencies": {
    "@types/react-dom": "^16.0.0",
    "classnames": "^2.2.5",
    "electron": "^1.7.8",
    "enzyme": "^2",
    "enzyme-adapter-react-16": "^1.0.1",
    "material-ui": "v1.0.0-beta.13",
    "material-ui-icons": "^1.0.0-beta.15",
    "material-ui-legacy": "npm:material-ui",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "next",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "typeface-roboto": "^0.0.35",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.0",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.32",
    "@types/react-hot-loader": "^3.0.4",
    "@types/react-intl": "^2.3.2",
    "@types/react-redux": "^5.0.10",
    "@types/react-router-dom": "^4.0.8",
    "@types/react-tap-event-plugin": "^0.0.30",
    "@types/recompose": "^0.24.2",
    "@types/redux-form": "^7.0.5",
    "@types/redux-mock-store": "^0.0.11",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.0",
    "@types/storybook__react": "^3.0.5",
    "@types/uuid": "^3.4.2",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.1",
    "awesome-typescript-loader": "^3.2.3",
    "clean-webpack-plugin": "^0.1.17",
    "codecov": "^2.3.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^21.2.1",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
