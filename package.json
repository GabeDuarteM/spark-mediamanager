{
  "name": "spark-mediamanager",
  "version": "0.1.0",
  "main": "src/main.js",
  "author": "Gabriel Duarte <gabrielqduarte@gmail.com> (https://github.com/GabrielDuarteM/)",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others --names \"WEBPACK,ELECTRON\" -s first \"yarn start:dev-server\" \"yarn start:electron\"",
    "start:electron": "cross-env NODE_ENV=development electron .",
    "start:dev-server": "webpack-dev-server --env.env=dev",
    "test:ci": "jest --coverage",
    "test:wcov": "jest --watch --coverage",
    "test:cov": "jest --coverage",
    "test": "jest --watch",
    "lint": "yarn lint:ts && yarn lint:prettier",
    "lint:ts": "tslint 'src/**/*.ts' 'src/**/*.tsx' '*.ts' '*.tsx' -e 'src/**/*.d.ts'",
    "lint:prettier:fix": "yarn lint:prettier --write",
    "lint:prettier": "prettier -l \"src/**/*.{tsx,ts,js}\" \".storybook/**/*.{tsx,ts,js}\" \"*.{tsx,ts,js}\"",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "webpack",
    "build:dev": "yarn build --env.env=dev",
    "build:prod": "yarn build --env.env=prod",
    "build:ci": "yarn test:ci && yarn lint && yarn build:prod && yarn build:dev && codecov",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "sb": "yarn storybook"
  },
  "jest": {
    "setupFiles": [
      "./src/utils/jest.setup.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "mapCoverage": true,
    "testURL": "http://localhost"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron": "^1.7.10",
    "material-ui": "v1.0.0-beta.26",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "typeface-roboto": "^0.0.50",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.9",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-intl": "^2.3.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/recompose": "^0.24.4",
    "@types/redux-form": "^7.0.12",
    "@types/redux-mock-store": "^0.0.13",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__react": "^3.0.6",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^3.8.2",
    "@types/webpack-env": "^1.13.3",
    "cache-loader": "^1.2.0",
    "codecov": "^3.0.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "hard-source-webpack-plugin": "^0.5.16",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.0",
    "jest": "^22.0.6",
    "prettier": "^1.10.2",
    "redux-mock-store": "^1.4.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.1",
    "thread-loader": "^1.1.2",
    "ts-checker-webpack-plugin": "^0.3.0",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.3.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.0",
    "webpack-dev-server": "^2.11.0"
  }
}
